    {{#each beer}}

    <div class="beer-container">
         <div class="left">
         <img src="{{this.image_url}}" alt="">

   </div>
  <div class="right">
    <h3>{{this.name}}</h3>
    <p>{{this.tagline}}</p>
    <p>{{this.description}}</p>
    <p>Food Pairing</p>
    <ul>
      {{#each this.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
  </div>

    </div>
    {{/each}}